        toastr.options = {
            "closeButton": true,
            "debug": false,
            "newestOnTop": false,
            "progressBar": false,
            "positionClass": "toast-bottom-right",
            "preventDuplicates": false,
            "onclick": null,
            "showDuration": "300",
            "hideDuration": "1000",
            "timeOut": "5000",
            "extendedTimeOut": "1000",
            "showEasing": "swing",
            "hideEasing": "linear",
            "showMethod": "fadeIn",
            "hideMethod": "fadeOut"
        }
    </script>
    <style>
        .sidebar #sidebar_menu > li a {
            grid-template-columns: 5px auto;
        }

        input:read-only {
            background: #a9b4d7
        }

        .single_crm {
            height: 100%;
        }
        /*.header_iner {
                    background: #e8ebf3;
                }*/
        .crm_bg_4 {
            background: #3b3b5c !important;
        }

        .crm_bg_5 {
            background: #f4ce2e !important;
        }

        .sidebar.mini_sidebar {
            width: 70px;
            min-width: 70px;
            max-width: 70px;
        }

        .main_content.full_main_content {
            padding-left: 70px;
        }

        .sidebar.mini_sidebar #sidebar_menu > li {
            width: 70px;
        }

        .sidebar.mini_sidebar .logo {
            width: 70px;
        }

        .sidebar #sidebar_menu > li a i {
            margin-right: -25px;
        }

        .sidebar_close_icon i {
            right: 0px;
            top: 10px;
        }

        .cs_modal .modal-header {
            background-color: #f7faff;
            padding: 10px 30px;
            border-bottom: 0 solid transparent;
        }

        .cs_modal {
            background-color: #ffffff;
            border: 0;
            box-shadow: 12px 1px 20px 0 rgb(0 0 0 / 25%), -8px -8px 12px 0 rgb(255 255 255 / 10%)
        }

        .footer_part p a {
            color: #865a40;
        }

        .footer_part .footer_iner.text-center {
            margin: -55px 15px;
        }

        .theme_bg_mainpage_modal_header {
            background: #a9b4d7 !important;
            /*background: linear-gradient(to bottom, #b3bcdf 0%, #959fbd 100%);*/
        }

        .profile_info .profile_info_iner {
            top: 43px;
        }

        #back-top a {
            background: #9ba4c1;
        }

        .cs_modal .modal-header h5 {
            font-weight: bolder;
        }

        input:read-only {
            background-color: #e8ecf1;
            cursor: not-allowed;
        }

        .k-widget {
            color: #343434;
            font-weight: bold;
        }

        read-only {
            background-color: #e8ecf1;
            cursor: not-allowed;
        }

        readonly {
            background-color: #e8ecf1;
            cursor: not-allowed;
        }

        .sorting_1 {
            color: black !important;
            font-weight: bold !important;
        }

        h1, h2, h3, h4, h5, h6 {
            color: #000000 !important;
        }

        @@media (min-width: 768px) {
            .table-responsive {
                overflow-x: hidden;
            }
        }

        @@media (max-width: 768px) {
            .table-responsive {
                overflow-x: auto;
            }
        }

        th {
            font-weight: bold !important;
            color: black !important
        }

        .profile_info .profile_info_iner .profile_author_name {
            padding: 4px 5px;
        }

        .profile_author_name > a {
            background: cornflowerblue;
        }

            .profile_author_name > a:hover {
                background: linear-gradient(to bottom right,#863ee2,#571f9e)
            }

        .profile_info .profile_info_iner::before {
            top: -19px;
        }

        .user_crm_wrapper .single_crm .crm_body p {
            color: #000000;
            font-weight: bold;
        }

        .user_crm_wrapper .single_crm {
            box-shadow: 12px 12px 16px 0 rgb(0 0 0 / 25%), -8px -8px 12px 0 rgb(255 255 255 / 30%);
        }

        .profile_info .profile_info_iner {
            margin-top: -15px !important;
        }

            .profile_info .profile_info_iner::before {
                right: 7px;
            }

        .modal.show .modal-dialog {
            box-shadow: 5px 3px 5px #95bdc6;
        }

        .k-widget {
            border-color: #a9b3d0;
        }

        .k-autocomplete, .k-dropdown-wrap.k-state-default, .k-numeric-wrap.k-state-default, .k-picker-wrap.k-state-default {
            border-color: #a9b3d0;
        }

        @@font-face {
            font-family: solaimanLipi;
            src: url('../../Assets/lib/kendoUI/Masters/assets/fonts/SolaimanLipi_20-04-07.ttf') format('truetype');
        }
        *{
            font-family: solaimanLipi;
        }
    </style>
